{
  "name": "micayael/php-extras",
  "type": "library",
  "description": "Funciones extras sobre el core de php",
  "keywords": ["array_filter", "array_filter_recursive"],
  "homepage": "https://github.com/micayael/php-extras",
  "license": "MIT",
  "authors": [
    {
      "name": "Juan Ardissone (micayael)",
      "email": "micayael@gmail.com"
    }
  ],
  "require": {
    "php": ">=5.3"
  },
  "require-dev": {
    "phpunit/phpunit": "^7",
    "symfony/phpunit-bridge": "~2.7",
    "symfony/var-dumper": "^3.0",
    "friendsofphp/php-cs-fixer": "^2.11"
  },
  "autoload": {
    "files": [
      "Resources/functions/array_functions.php",
      "Resources/functions/date_functions.php",
      "Resources/functions/string_functions.php",
      "Resources/functions/html_functions.php"
    ],
    "psr-4": { "Micayael\\PHPExtras\\": "" }
  }
}
